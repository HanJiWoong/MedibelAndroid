<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <TextView
        android:id="@+id/TVCustomProgressTitle"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:text="기미"
        android:fontFamily="@font/notosanscjkkr_bold"
        android:textSize="14sp"
        android:textColor="@color/text_1"
        android:gravity="center"
        android:includeFontPadding="false"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintWidth_percent="0.163636364"/>


    <androidx.appcompat.widget.AppCompatImageView
        android:id="@+id/IVCustomProgressIcon"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:src="@drawable/ic_analysis_result_value_good"
        android:scaleType="center"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintDimensionRatio="h, 1:1" />

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/CLCustomProgressBg"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:background="#f4f4f4"
        app:layout_constraintDimensionRatio="h,40:3"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintWidth_percent="0.606060606">

        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/CLCustomProgressContent"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:backgroundTint="@null"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintWidth_percent="0.5" />
    </androidx.constraintlayout.widget.ConstraintLayout>


    <TextView
        android:id="@+id/TVCustomProgressValue"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="50"
        android:fontFamily="@font/notosanscjkkr_regular"
        android:textSize="12sp"
        android:textColor="#ee7700"
        android:gravity="center"
        android:includeFontPadding="false"
        app:layout_constraintStart_toStartOf="@id/CLCustomProgressBg"
        app:layout_constraintEnd_toEndOf="@id/CLCustomProgressBg"
        app:layout_constraintBottom_toTopOf="@id/CLCustomProgressBg"
        app:layout_constraintHorizontal_bias="0.5"/>

    <TextView
        android:id="@+id/TVCustomProgressMin"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="0"
        android:fontFamily="@font/notosanscjkkr_regular"
        android:textSize="12sp"
        android:textColor="#999999"
        android:gravity="center"
        android:includeFontPadding="false"
        app:layout_constraintStart_toStartOf="@id/CLCustomProgressBg"
        app:layout_constraintBottom_toTopOf="@id/CLCustomProgressBg"
        app:layout_constraintHorizontal_bias="0.5"/>

    <TextView
        android:id="@+id/TVCustomProgressMax"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="100"
        android:fontFamily="@font/notosanscjkkr_regular"
        android:textSize="12sp"
        android:textColor="#999999"
        android:gravity="center"
        android:includeFontPadding="false"
        app:layout_constraintEnd_toEndOf="@id/CLCustomProgressBg"
        app:layout_constraintBottom_toTopOf="@id/CLCustomProgressBg"
        app:layout_constraintHorizontal_bias="0.5"/>


</androidx.constraintlayout.widget.ConstraintLayout>